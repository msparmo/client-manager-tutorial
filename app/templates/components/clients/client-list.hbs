<h2 class="sub-header">Client List</h2>
<div class="table-responsive">
  <table class="table table-striped">
	  <thead>
	  	<tr>
	    	<th>First Name</th>
	      <th>Last Name</th>              
	    </tr>
	  </thead>
	  <tbody>
		{{#each model as |client|}}
			<tr class="client">
				<td>{{client.firstName}}</td> 
				<td>{{client.lastName}}</td>
			</tr>
		{{/each}}
		</tbody>
	</table>
</div>